def tri(len): 
    turtle.color('black')
    turtle.begin_fill()            
    for i in range(3):
        turtle.forward(len)
        turtle.left((1/3)*360)
        if len > 10:
            turtle.end_fill()
            tri(len/2)                                
              
              

