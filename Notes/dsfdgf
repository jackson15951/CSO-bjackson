def squ(len):             
    for i in range(3):
        if len > 10:
            squ(len/3)
            
        turtle.forward(len)
        turtle.left(90)
        turtle.forward(len)
        turtle.left(90)
        
        
            
        for i in range(4):
            
            turtle.left(90)
            turtle.forward(len)
            
        
        
        turtle.left(90)
        turtle.forward(len)
        
        
            
        for i in range(4):
            
            turtle.forward(len)
            turtle.left(90) 
         
         if len > 10:
            squ(len/3)   
         
         
