def fac(len):
    while True:
        for i in range(5):
            for i in range(5):
                turtle.left((1/5)*360)
                turtle.forward(len)
                turtle.left((4/5)*360)
                turtle.forward(len)
                    
            turtle.left((3/5)*360)
        
        turtle.left((4/5)*360)
        turtle.forward(len)    
        fac(len/2)
        

def fac(len,size):
    while True:
        for i in range(5):
            for i in range(5):
                turtle.forward(len)
                turtle.left((1/5)*360)
                turtle.forward(len)
                turtle.left((4/5)*360)

            turtle.left((1/5)*360)
            turtle.forward(size)
            turtle.left((1/5)*360)
            turtle.forward(size)
           
              
