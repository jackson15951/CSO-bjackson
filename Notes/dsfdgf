def squ(len):         
    for i in range(4):
        for i in range(2):
            turtle.color('black')
            turtle.begin_fill() 
            if len > 5:
                squ(len/3)
                
            for i in range(4):
                turtle.forward(len)
                turtle.left(90)
            
            turtle.end_fill()
            turtle.forward(len)
            
