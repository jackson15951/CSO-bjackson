def fac(len,size):
    while True:
        for i in range(5):
            for i in range(5):
                for i in range(5):
                    turtle.forward(len)
                    turtle.left((1/5)*360)
                    

                turtle.left((1/5)*360)
                turtle.forward(size)
                turtle.left((1/5)*360)
                turtle.forward(size)
            
            turtle.left((1/5)*360)
            turtle.forward(size/2)
            
              
        fac(len/2,size)


def fac(len):
    a = 60
    while True:
        for i in range(5):
            for i in range(5):
                turtle.forward(len)
                turtle.left((1/5)*360)
                

            if len > 2:
                turtle.left((2/5)*360)
                fac(len/2)
            else:
                turtle.left((1/5)*360)
                len = a  
                  
        if len > 2:
            turtle.left((2/5)*360)
            turtle.forward(len)
            fac(len/2)
        else:
            turtle.left((1/5)*360)
            len = a  


