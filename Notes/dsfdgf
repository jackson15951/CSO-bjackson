def squ(len):         
    for i in range(4):
        turtle.forward(len/3)
        for i in range(2): 
            if len > 20:
                squ(len/3)  
            for i in range(4):
                turtle.forward(len/3)
                turtle.left(90) 
            turtle.forward(len/3)
        turtle.left(90)
        
        
        turtle.color('black')
        turtle.begin_fill()
        
