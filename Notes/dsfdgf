'''    
turtle.speed(50)


def fac(len):             
    for i in range(5):
        turtle.forward(len)
        turtle.left((4/5)*360)
        if len > 5:
            fac(len/2)
    

def squ(len):         
    for i in range(4):
        turtle.forward(len/3)
        for i in range(2): 
            if len > 20:
                squ(len/3)  
            for i in range(4):
                turtle.forward(len/3)
                turtle.left(90) 
            turtle.forward(len/3)
        turtle.left(90)
        
                
        
def tri(len):             
    for i in range(3):
        turtle.forward(len)
        turtle.left((1/3)*360)
        if len > 5:
            tri(len/2)                                
              
