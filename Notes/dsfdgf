    if len < 10:
        turtle.color('black')
        turtle.begin_fill()
    
        for i in range(4):
            turtle.forward(len)
            turtle.left(90) 
        turtle.end_fill()
        
    else:         
        for i in range(4):
            
            for i in range(2):
                squ(len/3)
                turtle.forward(len/3)        
            
            turtle.forward(len/3)
            turtle.left(90)
     
